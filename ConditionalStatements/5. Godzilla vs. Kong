using System;
class Program
{
    static void Main(string[] args)
    {
        double filmBudget = double.Parse(Console.ReadLine());
        double numberExtras = double.Parse(Console.ReadLine());
        double priceClothesPerExtrra = double.Parse(Console.ReadLine());

        double filmDirector = filmBudget * 0.1;
        double priceClothes = numberExtras * priceClothesPerExtrra;
        if (numberExtras >= 150)
        {
            priceClothes = priceClothes * 0.9;
        }
        double total = filmDirector + priceClothes;
        if (filmBudget < total)
        {
            double needs = total - filmBudget;
            Console.WriteLine("Not enough money!");
            Console.WriteLine($"Wingard needs {needs:f2} leva more.");
        }
        else if (filmBudget >= total)
        {
            double left = filmBudget - total;
            Console.WriteLine("Action!");
            Console.WriteLine($"Wingard starts filming with {left:f2} leva left.");
        }
    }
}
